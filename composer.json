{
    "name": "planetoftheweb/flex-videos",
    "description": "A professional WordPress plugin for displaying flexible, responsive video embeds with YouTube API integration",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Ray Villalobos",
            "email": "ray@planetoftheweb.com"
        }
    ],
    "keywords": [
        "wordpress",
        "plugin",
        "video",
        "youtube",
        "responsive",
        "embed"
    ],        "homepage": "https://github.com/planetoftheweb/flex-videos",
    "support": {
        "issues": "https://github.com/planetoftheweb/flex-videos/issues",
        "source": "https://github.com/planetoftheweb/flex-videos"
    },
    "require": {
        "php": ">=7.4",
        "yahnis-elsts/plugin-update-checker": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "wp-coding-standards/wpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7"
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress .",
        "phpcbf": "phpcbf --standard=WordPress ."
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
